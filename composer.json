{
    "name": "your-name-here/xlsxview",
    "description": "XlsxView class for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "phpoffice/phpspreadsheet": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Xlsxview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xlsxview\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
